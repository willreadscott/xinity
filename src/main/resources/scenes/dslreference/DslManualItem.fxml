<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="root" minHeight="-Infinity" minWidth="-Infinity" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng302.controllers.dslreference.DslManualItemController">
   <children>
      <VBox fx:id="manualVBox" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Text fx:id="name" strokeType="OUTSIDE" strokeWidth="0.0" text="name">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Text>
            <Separator prefWidth="200.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="8.0" top="8.0" />
               </padding>
            </Separator>
            <Text fx:id="synopsis" strokeType="OUTSIDE" strokeWidth="0.0" text="synopsis">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <font>
                  <Font name="Monospaced Regular" size="12.0" />
               </font>
            </Text>
            <Separator prefWidth="200.0">
               <padding>
                  <Insets bottom="8.0" top="8.0" />
               </padding>
            </Separator>
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
